<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<listener>
		<listener-class>
			uk.ac.ox.oucs.iam.servlet.ApplicationServletContextListener
		</listener-class>
	</listener>
	<servlet>
		<servlet-name>KeyUtilitiesServlet</servlet-name>
		<servlet-class>uk.ac.ox.oucs.iam.servlet.KeyUtilitiesServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProjectRoleServlet</servlet-name>
		<servlet-class>uk.ac.ox.oucs.iam.servlet.ProjectRoleServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReceivePost</servlet-name>
		<servlet-class>uk.ac.ox.oucs.iam.servlet.ReceivePost</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AccessServlet</servlet-name>
		<servlet-class>uk.ac.ox.oucs.iam.servlet.AccessServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AccessServlet</servlet-name>
		<url-pattern>/AccessServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>KeyUtilitiesServlet</servlet-name>
		<url-pattern>/KeyUtilitiesServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProjectRoleServlet</servlet-name>
		<url-pattern>/ProjectRoleServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReceivePost</servlet-name>
		<url-pattern>/ReceivePost</url-pattern>
	</servlet-mapping>
</web-app>
